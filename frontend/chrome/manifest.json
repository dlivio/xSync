{
  "name": "xSync",
  "version": "0.3.3",
  "description": "Sync and manage tabs between different browsers.",
  "homepage_url": "http://menino.eu/",
  "manifest_version": 2,
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "xSync",
    "default_popup": "tabs.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": { "16": "/icons/planet-space16.png",
    "48": "/icons/planet-space48.png",
   "128": "/icons/planet-space.png" }
  
}
